    " import numpy as np\n",
    " n_samples = 1000\n",
    " a=np.random.rand(n_samples)-1\n",
    " b=np.random.rand(n_samples)\n",
    " a1=np.tile(a,(n_samples,1))\n",
    " b1=np.transpose(np.tile(b,(n_samples,1)))\n",
    " prob = np.sum(a1>b1)/(n_samples**2)\n",
    " print(prob)"
   ]
